<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<connector name="HDFS">
    <properties>
    <connectivity-support>false</connectivity-support>
    <OAuth-authentication>false</OAuth-authentication>
    <metadata>none</metadata>
    <datasense-enabled>false</datasense-enabled>
    <transformer>false</transformer>
  </properties>
    <simpleConfig>
        <required caption="Name" group="Basic Settings" javaType="java.lang.String" name="name" type="name">config</required>
        <required caption="Default File System Name" group="General" javaType="java.lang.String" name="defaultFileSystemName" prefix="hdfs" type="string"></required>
    </simpleConfig>
    <message-processors>
        <read name="Read from path">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Path" group="General" javaType="java.lang.String" name="path" type="string">foo/bar.txt</required>
            </attributes>
            <childElements/>
        </read>
        <get-metadata name="Get path meta data">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Path" group="General" javaType="java.lang.String" name="path" type="string">foo/bar.txt</required>
            </attributes>
            <childElements/>
        </get-metadata>
        <write name="Write to path">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Path" group="General" javaType="java.lang.String" name="path" type="string">foo/bar.txt</required>
            </attributes>
            <childElements/>
        </write>
        <append name="Append to path">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Path" group="General" javaType="java.lang.String" name="path" type="string">foo/bar.txt</required>
            </attributes>
            <childElements/>
        </append>
        <delete-file name="Delete file">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Path" group="General" javaType="java.lang.String" name="path" type="string">foo/bar.txt</required>
            </attributes>
            <childElements/>
        </delete-file>
        <delete-directory name="Delete directory">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Path" group="General" javaType="java.lang.String" name="path" type="string">foo/</required>
            </attributes>
            <childElements/>
        </delete-directory>
        <make-directories name="Make directories">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Path" group="General" javaType="java.lang.String" name="path" type="string">foo/bar</required>
            </attributes>
            <childElements/>
        </make-directories>
    </message-processors>
</connector>
